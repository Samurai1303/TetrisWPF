    <None Remove="Assets\Images\TileEmpty.png" />
    <None Remove="Assets\Images\TileGreen.png" />
    <None Remove="Assets\Images\TileOrange.png" />
    <None Remove="Assets\Images\TilePurple.png" />
    <None Remove="Assets\Images\TileRed.png" />
    <None Remove="Assets\Images\TileYellow.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Fonts\MachineGunk-nyqg.ttf" />
    <Resource Include="Assets\Images\Background.jpg" />
